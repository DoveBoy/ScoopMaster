{
    "version": "3.6",
    "description": "Interactive 6502 Disassembler for Atari binary files on Windows",
    "homepage": "https://www.atarimax.com/dis6502/",
    "license": "Freeware",
    "url": "https://downloads.sourceforge.net/project/dis6502/dis6502/3.6/dis6502-3.6.zip",
    "hash": "sha1:041cd64896786cb4435e8e128ab3265ad1282ae1",
    "pre_install": "if (!(Test-Path \"$persist_dir\\dis6502.ini\")) { New-Item \"$dir\\dis6502.ini\" | Out-Null }",
    "bin": "dis6502.exe",
    "shortcuts": [
        [
            "dis6502.exe",
            "Dis6502"
        ]
    ],
    "persist": "dis6502.ini",
    "checkver": {
        "sourceforge": "dis6502",
        "regex": "dis6502-([\\d.]+).zip"
    },
    "autoupdate": {
        "url": "https://downloads.sourceforge.net/project/dis6502/dis6502/$version/dis6502-$version.zip"
    }
}
