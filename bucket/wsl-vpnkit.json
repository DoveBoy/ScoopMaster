{
    "version": "0.4.0",
    "description": "Provide network connectivity to the WSL2 VM while connected to VPNs on the Windows host",
    "homepage": "https://github.com/sakai135/wsl-vpnkit",
    "depends": "srsrns/wslctl",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://ghproxy.com/https://github.com/sakai135/wsl-vpnkit/releases/download/v0.4.0/wsl-vpnkit.tar.gz#/wsl-vpnkit.pack",
            "hash": "93d378ad0fac8af8f0ec76d0052221ac954242fba30b54605880cfc2444d8a95"
        }
    },
    "checkver": "github",
    "installer": {
        "script": [
            "Get-Item \"$dir\\wsl-vpnkit.pack\" | Rename-Item -NewName \"$dir\\wsl-vpnkit.tar.gz\"",
            "wslctl create \"$dir\\wsl-vpnkit.tar.gz\" \"sakai/wsl-vpnkit:$version\" --no-user --v2"
        ]
    },
    "uninstaller": {
        "script": "wslctl remove \"wsl-vpnkit\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://ghproxy.com/https://github.com/sakai135/wsl-vpnkit/releases/download/v$version/wsl-vpnkit.tar.gz#/wsl-vpnkit.pack",
                "extract_dir": "Wsl-Vpnkit-$version-win64"
            }
        },
        "hash": {
            "url": "$baseurl/wsl-vpnkit.tar.gz.sha256"
        }
    },
    "_from": "okibcn/ScoopMaster"
}
